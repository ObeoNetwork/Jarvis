<plugin>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated workflow -->
      <factory
            uri="http://www.obeonetwork.org/dsl/jarvis/workflow"
            class="org.obeonetwork.jarvis.workflow.provider.WorkflowItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated workflow -->
      <factory
            uri="http://www.obeonetwork.org/dsl/jarvis/workflow/description"
            class="org.obeonetwork.jarvis.workflow.description.provider.DescriptionItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>

   <extension point="org.eclipse.emf.edit.childCreationExtenders">
      <!-- @generated workflow -->
      <extender
            uri="http://www.eclipse.org/sirius/description/1.1.0"
            class="org.obeonetwork.jarvis.workflow.description.provider.DescriptionItemProviderAdapterFactory$DescriptionChildCreationExtender"/>
   </extension>

</plugin>
